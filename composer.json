{
  "name": "alpdesk/alpdesk-parallax",
  "description": "Parallax for Contao 4",
  "keywords": [
    "contao",
    "alpdesk",
    "Parallax"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/xprojects-de/alpdesk-parallax",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "XProjects",
      "homepage": "https://x-projects.de",
      "email": "infos@x-projects.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/xprojects-de/alpdesk-parallax/issues"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.13 || ^5.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "contao/manager-bundle": "^4.13 || ^5.0 ",
    "contao/manager-plugin": "^2.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "slam/phpstan-extensions": "^6.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.4 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Alpdesk\\AlpdeskParallax\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Alpdesk\\AlpdeskParallax\\ContaoManager\\Plugin",
    "contao-component-dir": "public"
  },
  "config": {
    "contao-component-dir": "public",
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "contao-community-alliance/composer-plugin": true,
      "contao-components/installer": true,
      "contao/manager-plugin": true
    }
  }
}
